{
	"name": "node-triptempo-server",
	"version": "1.0.0",
	"description": "Rest API server for Trip Tempo App",
	"main": "index.js",
	"repository": "https://github.com/baguilar6174/node-triptempo-server.git",
	"keywords": [
		"node",
		"console",
		"clean",
		"architecture",
		"rest",
		"api"
	],
	"homepage": "https://github.com/baguilar6174/node-triptempo-server#readme",
	"author": "Bryan Aguilar",
	"license": "MIT",
	"scripts": {
		"test": "jest",
		"test:watch": "jest --watchAll",
		"test:coverage": "jest --coverage",
		"dev": "ts-node-dev --respawn --clear --transpile-only --ignore-watch node_modules ./src/app.ts",
		"build": "rimraf ./dist && tsc && yarn prisma:migrate:prod",
		"start": "yarn build && node dist/app.js",
		"prisma:migrate:prod": "prisma migrate deploy"
	},
	"devDependencies": {
		"@types/compression": "^1.7.5",
		"@types/express": "^4.17.21",
		"@types/jest": "^29.5.12",
		"@types/node": "^20.11.30",
		"@typescript-eslint/eslint-plugin": "^6.4.0",
		"eslint": "^8.0.1",
		"eslint-config-prettier": "^9.1.0",
		"eslint-config-standard-with-typescript": "^43.0.1",
		"eslint-plugin-import": "^2.25.2",
		"eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-promise": "^6.0.0",
		"jest": "^29.7.0",
		"prettier": "^3.2.5",
		"prisma": "^5.11.0",
		"rimraf": "^5.0.5",
		"ts-jest": "^29.1.2",
		"ts-node-dev": "^2.0.0",
		"typescript": "*"
	},
	"dependencies": {
		"@prisma/client": "^5.11.0",
		"compression": "^1.7.4",
		"dotenv": "^16.4.5",
		"env-var": "^7.4.1",
		"express": "^4.19.2",
		"express-rate-limit": "^7.2.0"
	}
}
